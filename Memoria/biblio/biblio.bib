journal = {Chemical Engineering Science},
keywords = {Gas-liquid systems,Interfacial area,Mass transfer coefficient,Prediction,Stirred tank reactors,Volumetric mass transfer coefficient},
number = {12},
pages = {2489--2501},
title = {{Theoretical prediction of gas-liquid mass transfer coefficient, specific area and hold-up in sparged stirred tanks}},
volume = {59},
year = {2004}
}
