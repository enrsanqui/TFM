journal = {INTERNATIONAL JOURNAL OF CHEMICAL REACTOR ENGINEERING Review},
keywords = {SCW,biomass,exergy analysis,gasification,review},
title = {{Biomass Gasification in Supercritical Water – A Review}},
volume = {7},
year = {2009}
}
