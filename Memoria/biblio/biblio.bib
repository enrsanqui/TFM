journal = {Chemical Engineering Science},
keywords = {Coalescence,Kinetic energy per unit mass,Liquid/liquid interface,Surfactant,Vorticity},
pages = {215--227},
publisher = {The Authors},
title = {{On the effect of surfactants on drop coalescence at liquid/liquid interfaces}},
