isbn = {0009-2509},
issn = {1369703X},
journal = {Biochemical Engineering Journal},
keywords = {Gas-liquid systems,Interfacial area,Mass transfer coefficient,Non-Newtonian liquids,Oxygen transfer,Prediction,Stirred tank reactor,Stirred tank reactors,Volumetric mass transfer coefficient},
